extends layout

mixin message(opp, name)
    if(opp)
        h1.greeting welcome board
            span=name 



append links 
    li(style="float: right;")
        a(href="/logout") Logout 
    
block content
    .dashboard 
        .user 
            +message(opp, name)
    .mypage-panel
        include includes/mypage-form
    .mypage-panel 
        include includes/withdrawal-form


